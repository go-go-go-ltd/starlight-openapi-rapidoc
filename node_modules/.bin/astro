#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lewis.jacklin/starlight-openapi-rapidoc/node_modules/.pnpm/astro@4.4.11_@types+node@20.11.24/node_modules/astro/node_modules:/Users/lewis.jacklin/starlight-openapi-rapidoc/node_modules/.pnpm/astro@4.4.11_@types+node@20.11.24/node_modules:/Users/lewis.jacklin/starlight-openapi-rapidoc/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lewis.jacklin/starlight-openapi-rapidoc/node_modules/.pnpm/astro@4.4.11_@types+node@20.11.24/node_modules/astro/node_modules:/Users/lewis.jacklin/starlight-openapi-rapidoc/node_modules/.pnpm/astro@4.4.11_@types+node@20.11.24/node_modules:/Users/lewis.jacklin/starlight-openapi-rapidoc/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
